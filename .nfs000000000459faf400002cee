#include "my_pthread_t.h"
#include "my_pthread.c"

void * func1(void * arg){
  printf("I'm function 1\n");
}

void * func2(void * arg){
  printf("I'm function 2\n");
}

int main(int argc, char *argv[]){
  pthread_attr_t attr;
  struct thread_info *arg;
  int x = my_pthread_create(0,&attr,&func1,&arg);
  printf("-%d-\n",x);
  printf("Done\n");
  return 0;
}
